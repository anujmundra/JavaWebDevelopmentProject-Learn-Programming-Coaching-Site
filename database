CREATE TABLE `batch` (
  `bcode` varchar(20) DEFAULT NULL,
  `plang` varchar(50) DEFAULT NULL,
  `fname` varchar(50) DEFAULT NULL,
  `sdate` varchar(20) DEFAULT NULL,
  `duration` varchar(20) DEFAULT NULL,
  `edate` varchar(20) DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1

CREATE TABLE `courses` (
  `courseid` varchar(10) NOT NULL,
  `ctitle` varchar(20) DEFAULT NULL,
  `cdesc` varchar(50) DEFAULT NULL,
  `duration` int(11) DEFAULT NULL,
  `fees` int(11) DEFAULT NULL,
  PRIMARY KEY (`courseid`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1

CREATE TABLE `faculty` (
  `name` varchar(50) DEFAULT NULL,
  `cname` varchar(50) DEFAULT NULL,
  `experience` varchar(10) DEFAULT NULL
) ENGINE=InnoDB DEFAULT CHARSET=latin1

CREATE TABLE `users` (
  `userid` varchar(10) NOT NULL,
  `password` varchar(10) DEFAULT NULL,
  `uname` varchar(15) DEFAULT NULL,
  `address` varchar(20) DEFAULT NULL,
  `mobile` varchar(12) DEFAULT NULL,
  `email` varchar(25) DEFAULT NULL,
  PRIMARY KEY (`userid`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1
